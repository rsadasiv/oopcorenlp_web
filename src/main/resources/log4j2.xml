<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36}:%L - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console"/>
    </Root>
    <Logger name="edu.stanford.nlp.quoteattribution" level="error" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="com.vader" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="org.apache.http" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
        <Logger name="org.apache.http.wire" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
        <Logger name="org.apache.http.headers" level="info" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="io.outofprintmagazine" level="debug" additivity="false">
      <AppenderRef ref="Console"/>
    </Logger>
  </Loggers>
</Configuration>